package PracticeTask0316;

public class ArrayPractice2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String [] [] c = {
				{"Mr", "Mrs", "Ms", "Miss"},
				{"Smith", "Jordan", "Jackson", "Obama"},
				
		};
		
		System.out.print(c[0][0] + " " );
		System.out.println(c[1][0]);
	
		double[][] a = {
				{1.4,2.0,3.3,2},
				{4,1.5,6.1,1},
				{1.2,3.1,4,1.6}
			};
			
		
		double sum=0.0;
		for (double i=0.0; i<a.length; i++){
		   for (double b=0.0; b<a.length[i];b++)
		   sum = sum + a[i][b];
		}
	System.out.println();
	
	}

}
