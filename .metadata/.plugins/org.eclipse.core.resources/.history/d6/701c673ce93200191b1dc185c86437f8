package com.variables;

public class ArithmeticOperations {
public static void main (String [] args) {

	//Arithmetic
	int a=20;
	int b=5;
	
	int sum=a+b;
	int sub=a-b;
	int multi=a*b;
	int div=a/b;
	System.out.println(sum);
	System.out.println(sub);
	System.out.println(multi);
	System.out.println(div);
}
}
